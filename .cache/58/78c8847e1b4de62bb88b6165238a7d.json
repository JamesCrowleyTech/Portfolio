avy: rgb(36, 37, 42);\n\n$font-size-massive: 4rem;\n$font-size-huge: 2.8rem;\n$font-size-very-large: 2.2rem;\n$font-size-large: 1.9rem;\n$font-size-normal: 1.6rem;\n\n$font-darkest: rgb(15, 15, 15);\n$font-dark: rgb(37, 37, 37);\n$font-medium: rgb(66, 66, 66);\n$font-light: rgb(90, 90, 90);\n$font-lightest: rgb(233, 233, 233);\n","src/sass/base/_base.scss":"* {\n    margin: 0;\n    padding: 0;\n}\n\n*,\n*::before,\n*::after {\n    box-sizing: inherit;\n}\n\nhtml {\n    box-sizing: border-box;\n    font-size: 62.5%;\n}\n\nbody {\n    font-size: 1.6rem;\n    font-family: $font-primary;\n    color: $font-dark;\n    //width: 100vw;\n    position: absolute;\n}\n\n.container {\n    display: grid;\n    grid-template-rows: 7rem;\n}\n","src/sass/components/_btn.scss":".btn {\n    text-decoration: none;\n    padding: 1.2rem 2rem 0.9rem 2rem;\n    background-color: white;\n\n    display: flex;\n    align-items: center;\n    line-height: 1;\n    font-size: $font-size-large;\n    font-weight: 400;\n    transition: all 0.2s;\n\n    &__normal {\n        border-radius: 100rem;\n        &:hover {\n            color: $color-primary;\n        }\n    }\n\n    &__fill-on-hover {\n        border-radius: 100rem;\n        border: 2px solid $color-primary;\n        transform: translateY(-2px);\n        color: $color-primary;\n\n        &:hover {\n            background-color: $color-primary;\n            color: white;\n        }\n    }\n\n    &__intro {\n        background-color: darken(\n            rgba($color: $color-primary, $alpha: 0.45),\n            10%\n        );\n        padding: 2rem 4rem;\n        color: white;\n        display: inline-block;\n        border-radius: 999rem;\n        font-size: $font-size-very-large;\n        &:hover {\n            background-color: darken(\n                rgba($color: $color-primary, $alpha: 1),\n                10%\n            );\n        }\n    }\n}\n","src/sass/components/_main-icon.scss":".main-icon {\n    display: flex;\n    width: 6rem;\n    height: 6rem;\n    align-items: center;\n    justify-content: center;\n    img {\n        display: inline-block;\n        height: 100%;\n        width: 100%;\n        transform: scaleY(0.92);\n    }\n}\n","src/sass/components/_work-figure.scss":".work-figure {\n    transition: all 0.3s;\n    position: relative;\n    overflow: hidden;\n    border-radius: 1.5rem;\n    //box-shadow: 0px 5px 15px 8px rgba(0, 0, 0, 0.2);\n\n    &__start {\n        top: 0;\n        left: 0;\n        height: 100%;\n        width: 100%;\n    }\n\n    &__img {\n        transition: all 0.3s;\n        width: 100%;\n        height: 100%;\n        object-fit: cover;\n        border-radius: 1.5rem;\n        //display: inline-block;\n    }\n\n    // &::after {\n    //     content: \"\";\n    //     width: 100%;\n    //     height: 100%;\n    //     display: inline-block;\n    //     display: none;\n    //     box-shadow: 0 0 50px 50px #000000 !important;\n    //     position: absolute;\n    //     top: 0;\n    //     left: 0;\n    //     border-radius: inherit;\n    //     z-index: 100;\n    //     background-color: red;\n    // }\n\n    &:hover {\n        box-shadow: 0px 0px 18px 15px rgba(0, 0, 0, 0.32);\n    }\n\n    &:hover &__img {\n        transform: scale(1.15);\n    }\n\n    &:hover &__hovered {\n        opacity: 1;\n        visibility: visible;\n    }\n\n    &__figcaption {\n        font-size: $font-size-massive;\n        color: $font-lightest;\n        text-align: center;\n        letter-spacing: 2px;\n        line-height: 1;\n        background-color: whitesmoke;\n        position: absolute;\n        display: inline-block;\n        left: 0;\n        width: 100%;\n        z-index: 22;\n    }\n\n    &__hovered {\n        position: absolute;\n        top: 0;\n        left: 0;\n        height: 100%;\n        width: 100%;\n        background-color: rgba($color-navy, 0.85);\n        z-index: 10;\n        display: flex;\n        flex-direction: column;\n        justify-content: space-around;\n        padding: 2rem;\n        opacity: 0;\n        visibility: hidden;\n        transition: all 0.3s;\n    }\n\n    &__title {\n        width: 100%;\n        display: inline-block;\n        font-size: $font-size-huge;\n        font-weight: $font-medium;\n        color: $font-lightest;\n        text-align: center;\n        text-transform: uppercase;\n        letter-spacing: 1px;\n    }\n\n    &__description {\n        color: $font-lightest;\n        text-align: center;\n        font-size: $font-size-normal + 0.2rem;\n    }\n\n    &__buttons-wrapper {\n        display: flex;\n        justify-content: space-between;\n    }\n\n    // &__button:link,\n    &__button {\n        text-decoration: none;\n        display: flex;\n        justify-content: center;\n\n        width: 45%;\n        padding: 1.5rem;\n        border: none;\n        color: $font-lightest;\n        font-size: 2.1rem;\n        border-radius: 1000px;\n        white-space: nowrap;\n        background-color: crimson;\n        transition: all 0.15s;\n    }\n}\n","src/sass/layout/_nav.scss":".nav {\n    display: grid;\n    width: 100vw;\n    grid-template-columns: 1fr minmax(28rem, 95vw) 1fr;\n    grid-template-rows: min-content;\n    align-content: center;\n\n    div {\n        grid-column: 2 / 3;\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n    }\n\n    &__buttons {\n        list-style: none;\n        display: flex;\n\n        li:not(:first-child) {\n            margin-left: 1.5rem;\n        }\n    }\n\n    &__image {\n        height: 6rem;\n        width: 6rem;\n    }\n}\n","src/sass/layout/_intro.scss":".intro {\n    color: white;\n    background-color: rgba($color: $color-primary, $alpha: 0.75);\n    padding: 12rem;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    align-items: center;\n\n    & > * {\n        margin: 2rem 0;\n    }\n}\n","src/sass/layout/_work.scss":".work {\n    display: grid;\n    width: 